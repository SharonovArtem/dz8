using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;
using System.Threading.Tasks;

namespace jun1
{
    internal class Program
    {
        static void Main(string[] args)
        {
            string readLine = Console.ReadLine();
            string exit = "exit";
            
            while (readLine != exit)
            {
                Console.WriteLine("Hello World!");
                readLine = Console.ReadLine();
            }
        }
    }
}
